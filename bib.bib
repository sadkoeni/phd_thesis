author = {V.M. Grichine},
keywords = {Glauber approach, Nucleus-nucleus cross-section},
abstract = {A modified model for integral cross-sections of light nuclei based on the Glauber-Gribov representation is discussed. Two modifications were introduced to the model. The first is a more correct consideration of the nucleon elastic scattering amplitude. The second is using of root mean square radius of nucleon distribution in nuclei based on measurements of the electron-nucleus scattering. The model predictions are compared with experimental data for the inelastic nucleus-nucleus cross-sections.}
},
